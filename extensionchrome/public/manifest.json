{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "UNL SecureWeb",
  "icons": {
    "16": "/icono.png",
    "48": "/icono.png",
    "128": "/icono.png"
  },
  "description": "UNL SecureWeb, podrás navegar por internet con confianza, sabiendo que estás protegido contra enlaces potencialmente peligrosos.",
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "alarms",
    "notifications",
    "background"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "secureweb@unl.com",
      "strict_min_version": "91.0"
    }
  }
}
